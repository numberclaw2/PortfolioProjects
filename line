CREATE MULTISET VOLATILE TABLE
   LINE_PLAN_CHG, NO Log AS
 (
 SELECT DISTINCT
ACTIVITY_DT,
CUST_ID,
CUST_LINE_SEQ_ID,
PLAN_CHG_IND
FROM   NTL_PRD_QMTBLS.HQCS_NV_DETAIL
WHERE PLAN_CHG_IND = 'Y'
AND ACTIVITY_DT BETWEEN 1241001 AND 1241031
)
WITH DATA
 PRIMARY INDEX(ACTIVITY_DT , CUST_ID, CUST_LINE_SEQ_ID )
ON COMMIT
PRESERVE ROWS
;
